{
  "name": "miskatonic-client",
  "author": "Cooper Hammond",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "test": "jest"
  },
  "homepage": "https://cooperhammond.github.io/miskatonic-book-manager",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "es6-promise": "^4.2.5",
    "flux": "^3.1.3",
    "immutability-helper": "^2.9.0",
    "keymirror": "^0.1.1",
    "node-sass": "^4.13.1",
    "prop-types": "^15.6.2",
    "qrcode.react": "^0.9.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron": "^3.0.13",
    "foreman": "^3.0.1",
    "gh-pages": "^2.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.6.3"
  }
}
